{
    "name": "ghanem/multiple-date-picker",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ghanem\\MultipleDatePicker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ghanem\\MultipleDatePicker\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}